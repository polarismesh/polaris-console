{
  "name": "polaris-frontend",
  "version": "1.0.0",
  "description": "> 构建结果会打包到 ***polaris-frontend*** 模块中",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config build/webpack.dev.conf.js",
    "public-dev": "cross-env NODE_ENV=development BUILD_TYPE=public webpack serve --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js && bash build/copy-file.sh && tar -czvf ./polaris-frontend.tar.gz ./dist",
    "public-build": "cross-env NODE_ENV=production BUILD_TYPE=public webpack --config build/webpack.prod.conf.js && bash build/copy-file.sh && tar -czvf ./polaris-frontend.tar.gz ./dist"
  },
